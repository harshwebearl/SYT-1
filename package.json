{
  "name": "start-your-tour",
  "version": "1.0.0",
  "description": "Start Your Tour API - A comprehensive tour management system",
  "main": "./bin/www",
  "private": true,
  "keywords": [
    "tour",
    "travel",
    "api",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Harsh WebEarl",
  "license": "Harsh",
  "scripts": {
    "postinstall": "patch-package",
    "start": "node ./bin/www",
    "dev": "SET NODE_ENV=development & nodemon ./bin/www",
    "prod": "SET NODE_ENV=production & node ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "format": "npm run prettier:write && npm run lint:fix",
    "start-gendoc": "SET NODE_ENV=development & node swagger.js",
    "swagger": "node swagger.js"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "easyinvoice": "^3.0.47",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "express": "^4.21.1",
    "express-oas-generator": "^1.0.46",
    "firebase-admin": "^12.6.0",
    "http-errors": "^2.0.0",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.1",
    "mongoose-aggregate-paginate-v2": "^1.1.2",
    "mongoose-paginate-v2": "^1.8.3",
    "mongoose-to-swagger": "^1.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-input-validator": "^4.5.1",
    "nodemailer": "^6.9.15",
    "pdfjs-dist": "^3.4.120",
    "pdfkit": "^0.15.0",
    "quill-delta-to-html": "^0.12.1",
    "sha256": "^0.2.0",
    "socket.io": "^4.8.1",
    "swagger-autogen": "^2.23.7",
    "twilio": "^5.3.4",
    "uniqid": "^5.4.0",
    "uuid": "^10.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/start-your-tour.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/start-your-tour/issues"
  },
  "homepage": "https://github.com/yourusername/start-your-tour#readme"
}
